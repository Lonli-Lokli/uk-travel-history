{
  "name": "@uth/root",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "node scripts/check-git-config.js",
    "build": "nx run app:build",
    "start": "nx dev app",
    "format": "nx format:write",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop --no-backup",
    "db:reset": "npx supabase db reset --local",
    "db:types": "npx supabase gen types typescript --local --schema public > packages/db/src/internal/providers/supabase.types.ts",
    "db:refresh": "npm run db:reset && npm run db:types",
    "lint": "nx run-many --all --target=lint --fix=true --verbose",
    "test": "nx run-many --all --target=test --passWithNoTests --silent",
    "test:coverage": "nx run-many --all --target=test --coverage --passWithNoTests && node tools/fix-coverage-paths.js",
    "test:e2e": "nx run-many --all --target=e2e",
    "test:e2e:ui": "nx run-many --all --target=e2e --ui",
    "test:e2e:debug": "nx run-many --all --target=e2e --debug",
    "test:e2e:report": "playwright show-report",
    "update": "cross-env NX_SKIP_PROVENANCE_CHECK=true npx nx migrate latest && npm install && npx nx migrate --run-migrations",
    "deploy": "run-s test build"
  },
  "private": true,
